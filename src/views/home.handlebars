<main>
  {{! Menu desplegable }}

  <div class="flex justify-items-center place-content-center bg-gray-400 p-2">
    <div>
      <button
        id="dropdownDefaultButton"
        data-dropdown-toggle="dropdown"
        class="text-black font-bold focus:ring-4 focus:outline-none focus:ring-gray-800 rounded-lg text-sm px-4 py-2.5 text-center inline-flex items-center"
        type="button"
      >Filtrar por...
        <svg
          class="w-4 h-4 ml-2"
          aria-hidden="true"
          fill="none"
          stroke="currentColor"
          viewBox="0 0 24 24"
          xmlns="http://www.w3.org/2000/svg"
        >
          <path
            stroke-linecap="round"
            stroke-linejoin="round"
            stroke-width="2"
            d="M19 9l-7 7-7-7"
          ></path>
        </svg></button>
      <!-- Dropdown menu -->
      <div
        id="dropdown"
        class="z-10 hidden bg-white divide-y divide-gray-100 rounded-lg shadow w-44 bg-gray-700 absolute"
      >
        <ul
          class="py-2 text-sm text-gray-700 text-gray-200"
          aria-labelledby="dropdownDefaultButton"
        >
          <li>
            <a
              href="/?category=pantalones"
              class="block px-4 py-2 hover:bg-gray-100 hover:bg-gray-600 hover:text-white"
            >Pantalones</a>
          </li>
          <li>
            <a
              href="/?category=remeras"
              class="block px-4 py-2 hover:bg-gray-100 hover:bg-gray-600 hover:text-white"
            >Remeras</a>
          </li>
          <li>
            <a
              href="/?category=camisas"
              class="block px-4 py-2 hover:bg-gray-100 hover:bg-gray-600 hover:text-white"
            >Camisas</a>
          </li>
          <li>
            <a
              href="/?category=zapatos"
              class="block px-4 py-2 hover:bg-gray-100 hover:bg-gray-600 hover:text-white"
            >Zapatos</a>
          </li>
          <li>
            <a
              href="/"
              class="block px-4 py-2 hover:bg-gray-100 hover:bg-gray-600 hover:text-white"
            >Ver todos</a>
          </li>
        </ul>
      </div>
    </div>
    {{#if hasPrevPage}}
      <a
        href="/?page={{prevPage}}"
        class="inline-flex items-center px-4 py-2 mr-3 text-sm font-medium text-gray-500 border border-gray-300 rounded-lg hover:bg-gray-100 hover:text-gray-700 bg-gray-800 border-gray-700 text-gray-400 hover:bg-gray-700 hover:text-white"
      >
        <svg
          aria-hidden="true"
          class="w-5 h-5 mr-2"
          fill="currentColor"
          viewBox="0 0 20 20"
          xmlns="http://www.w3.org/2000/svg"
        >
          <path
            fill-rule="evenodd"
            d="M7.707 14.707a1 1 0 01-1.414 0l-4-4a1 1 0 010-1.414l4-4a1 1 0 011.414 1.414L5.414 9H17a1 1 0 110 2H5.414l2.293 2.293a1 1 0 010 1.414z"
            clip-rule="evenodd"
          ></path>
        </svg>
        Previous
      </a>
    {{/if}}

    {{#if hasNextPage}}
      <a
        href="/?page={{nextPage}}"
        class="inline-flex items-center px-4 py-2 text-sm font-medium text-gray-500 border border-gray-300 rounded-lg hover:bg-gray-100 hover:text-gray-700 bg-gray-800 border-gray-700 text-gray-400 hover:bg-gray-700 hover:text-white"
      >
        Next
        <svg
          aria-hidden="true"
          class="w-5 h-5 ml-2"
          fill="currentColor"
          viewBox="0 0 20 20"
          xmlns="http://www.w3.org/2000/svg"
        >
          <path
            fill-rule="evenodd"
            d="M12.293 5.293a1 1 0 011.414 0l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414-1.414L14.586 11H3a1 1 0 110-2h11.586l-2.293-2.293a1 1 0 010-1.414z"
            clip-rule="evenodd"
          ></path>
        </svg>
      </a>
    {{/if}}
  </div>
  {{! }}

  <section class="product relative overflow-hidden p-5">
    <button
      class="pre-btn bg-gradient-to-r from-transparent to-white absolute top-1/2 flex justify-center items-center border-none cursor-pointer z-10"
    >
      <i
        class="fa-solid fa-arrow-left fa-2xl opacity-20 hover:opacity-100 h"
      ></i>
    </button>

    <button
      class="nxt-btn bg-gradient-to-l from-transparent to-white h-full absolute top-0 right-0 flex justify-center items-center border-none cursor-pointer z-10"
    >
      <i
        class="fa-solid fa-arrow-right fa-2xl opacity-20 hover:opacity-100"
      ></i>
    </button>
    <div
      class="product-container overflow-x-auto no-scrollbar p-10 flex scroll-smooth"
    >
      {{#each products}}
        <div
          class="product-card flex-none w-56 h-auto mx-4 rounded-lg border border-gray-300 bg-white shadow-lg hover:shadow-2xl transition duration-500 cursor-pointer"
        >
          <div class="product-image relative w-full h-64">
            <!-- Rest of the image content -->
            <span
              class="discount-tag absolute top-4 right-4 bg-white p-2 rounded text-red-500 text-sm capitalize"
            >50% off</span>
            <img
              src={{this.thumbnail}}
              class="product-thumb w-full h-full object-cover"
              alt={{this.title}}
            />
            <button
              onclick="openModal(this)"
              data-id="{{this.id}}"
              class="absolute bottom-4 left-1/2 transform -translate-x-1/2 p-2 w-4/5 rounded capitalize opacity-0 hover:opacity-100 hover:bg-violet-400 transition duration-500 cursor-pointer bg-white text-gray-800 font-semibold"
            >add to cart</button>
          </div>
          <div class="product-info p-2">
            <h2
              class="product-brand text-uppercase font-bold text-lg"
            >{{this.title}}</h2>
            <p
              class="product-short-description truncate opacity-50 capitalize my-1 truncate-l"
            >{{this.description}}</p>
            <div class="price-container flex items-center">
              <span
                class="price font-semibold text-lg text-violet-500"
              >${{this.price}}</span>
              <span
                class="actual-price ml-4 text-gray-400 line-through"
              >${{this.price}}</span>
            </div>
          </div>
        </div>
      {{/each}}
      <!-- Add more product-card elements here for the slider -->
    </div>
